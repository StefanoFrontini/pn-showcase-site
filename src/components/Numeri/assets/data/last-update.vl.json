{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "name": "myData"
  },
  "transform": [{ "calculate": "toDate(datum.last_run)", "as": "run_date" }],
  "mark": "text",
  "encoding": {
    "text": { "field": "run_date", "type": "temporal", "format": "%d/%m/%Y" }
  }
}
